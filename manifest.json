{
  "manifest_version": 3,
  "name": "Personal Assistant",
  "version": "1.5",
  "description": "An AI-powered personal assistant that helps users with shopping, career opportunities, and news updates.",
  "permissions": ["history", "sidePanel"],
  "host_permissions": ["https://copilot.microsoft.com/"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
